{
  "title": "百度", // 软件标题, 为null空空字符串表示默认(下同), 默认取页面标题
  
  "width": 1200,
  "height": 800,

  "topmost": false,             // 置顶显示
  "disable_right_click": false, // 禁用鼠标右键(但保留复制,粘贴等操作)
  "disable_history": false,     // 禁用历史记录
  "enable_f12": true,           // 启用F12(打开网页调试)
  "enable_tray": true,          // 启用小托盘
  "minimize_to_tray": true,     // 最小化到推盘(必须先开启 enable_tray)
  "boss_key": "alt+q",          // 老板键
  "popuppage_target": "_self", // 弹出页面状态, _self, _blank(默认), _top

  "browser_loglevel": "error",  // 浏览器日志级别(default,verbose,info,warning,erroe,fatal,disable)
  //"url_filter": "^http[s]?://([^.]+[.])?(baidu[.]|sina[.])", // 页面地址过滤(未匹配的地址无法打开)
  "save_exited_location": true, // 退出时保存位置
  "url": "http://www.baidu.com" // 软件启动时显示的页面
}